{
  "expo": {
    "name": "dingdongbaby",
    "slug": "dingdongbaby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "To view photos in DingDong Baby give us photos permission. We NEVER store your beautiful photos off your device.",
          "cameraPermission": "Wanna take a quick snap of your baby? Please let DingDong Baby access your camera.",
          "microphonePermission": "Record those goo goo gah gahs for future you. Please let DingDong Baby access your mic."
        }
      ]
    ]
  }
}
